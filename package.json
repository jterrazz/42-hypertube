{
  "private": true,
  "scripts": {
    "lint": "eslint --fix --ext .js,.jsx,.ts app/server app/web",
    "dev.server": "cd server && nodemon",
    "dev.web": "next dev ./web",
    "build.web": "next build ./web",
    "start.web": "next start ./web",
    "start.server": "ts-node ./server/index.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "next": "9.0.4",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  }
}
